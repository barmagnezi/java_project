<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
  <hibernate-mapping>
      <class name="User" table="Mazes" >
          <id name="userId" type="java.lang.Long" column="user_id" >
                <generator class="increment" />
          </id>
          <property name="name" type="java.lang.String" column="name" length="-1" />
          <property name="maze" type="java.lang.String" column="maze" length="-1" />
          <property name="solution" type="java.lang.String" column="solution" length="-1" />
         
      </class>
  </hibernate-mapping>
